		<queryString language="SQL">
			<![CDATA[select *
from (
select row_number() over (order by a.id) as rn, a.id, to_char(c.payment_date,'dd.mm.yyyy'),c.payment_summ, c.int_summ, c.principal_summ, c.debt_balance  from  doc_cl a, reg_cr_abis b,  repaiment c
  where  b.id = a.reg_cr_abis2_id --nvl(a.reg_cr_abis2_id, a.reg_cr_abis_id)               
  and c.reg_cr_abis_id = b.id
  and a.id= $P{Идентификатор2} 
  
  union all
select null, a.id, 'Итого(Жиынтығы):',sum(c.payment_summ), sum(c.int_summ), sum(c.principal_summ), sum(c.debt_balance)
  from  doc_cl a, reg_cr_abis b,  repaiment c
  where  b.id = a.reg_cr_abis2_id --nvl(a.reg_cr_abis2_id, a.reg_cr_abis_id)
  and c.reg_cr_abis_id = b.id
  and a.id= $P{Идентификатор2} 
  group by a.id) q
order by q.rn nulls last   -- сортировка по возрастанию, пустые - в конце]]>
