              'августа',
              '09',
              'сентября',
              '10',
              'октября',
              '11',
              'ноября',
              '12',
              'декабря'),
       to_char(b.loan_date, 'yyyy') -- дата выдачи займа
       ,
       to_char(b.date_created, 'dd.MM.yyyy') as loandate1-- дата выдачи займа       
      ,
       to_char(b.date_updated, 'dd') -- день дата формирования графика
      ,
       decode(to_char(b.date_updated, 'mm'), --  месяц дата формирования графика
              '01',
              'января',
              '02',
              'февраля',
              '03',
              'марта',
              '04',
              'апреля',
              '05',
              'мая',
              '06',
              'июня',
              '07',
              'июля',
              '08',
              'августа',
              '09',
              'сентября',
              '10',
              'октября',
              '11',
              'ноября',
              '12',
              'декабря'),
       to_char(b.date_updated, 'yyyy') -- дата формирования графика
      ,
      dc.doc_number as doc_number -- номер договора (транша)
      ,b.loan_code
      ,up1.lastname || ' '||up1.firstname || ' ' || nvl(up1.middle_name, '')  as manager_fio
      ,
       to_char(sysdate, 'dd.mm.yyyy') as sddmy,-- день тек. даты
      nvl(b.loan_acc,' ') as acc ,-- тек счет клиента, если счет был выбран менеджером  dc.CHECK_ABIS_ACC_ID не пусто, то этот счет отразится в reg_cr_abis       
       dc.is_refinancing,
       'БИК: ALFAKZKA',
        to_char(sysdate, 'dd') -- день дата выдачи займа
      ,
       decode(to_char(sysdate, 'mm'), --  месяц дата выдачи займа
              '01',
              'января',
              '02',
              'февраля',
              '03',
              'марта',
              '04',
              'апреля',
              '05',
              'мая',
              '06',
              'июня',
              '07',
              'июля',
              '08',
              'августа',
              '09',
              'сентября',
              '10',
              'октября',
              '11',
              'ноября',
              '12',
              'декабря'),
       to_char(sysdate, 'yyyy'),
       (select distinct
               LISTAGG(pvc.bank, ', ') within group(order by pvc.doc_cl_id) as ref_bank
               from             (select distinct pvc.bank, pvc.doc_cl_id  
               from PKB_VALID_CREDITS pvc
               where pvc.is_refinance > 0) pvc
                    where pvc.doc_cl_id = dc.id
                    and pvc.bank <> 'АО ДБ "Альфа-Банк"'
               group by pvc.doc_cl_id) as ref_bank,

       to_char(sysdate, 'dd.mm.YYYY') as now_date
       
from doc_cl dc

LEFT JOIN (select *
                from (select a.assignee_, b.text_,a.end_time_, row_number() over (partition by b.text_ order by b.text_,a.end_time_ ) as rw
                        from act_hi_taskinst a
                          inner join act_ru_variable b on b.proc_inst_id_ = a.proc_inst_id_
                       where b.name_ = 'recordId'
                         and a.task_def_key_ = 'STAGE_NUMBER_210'
                        order by b.text_,a.end_time_ desc) w
               where w.rw = 1) bb
 
    on to_char(dc.id) = bb.text_

       left join USER_PROFILE  up on (bb.assignee_ = up.login)
       LEFT JOIN USER_PROFILE    up1  on up.manager_user_id = up1.id
       left join REF_CURRENCY rc on (dc.CR_CURRENCY_ID = rc.id)
       left join reg_cr_abis  b on (dc.reg_cr_abis_id = b.id)
       left join REF_BRANCH rb on (up1.branch_id=rb.id)
       left join REF_CITY rct on (rb.city_id=rct.id)
       left join REF_CR_PURPOSE     rp on (dc.CR_PURPOSE_ID = rp.id)
       left join REF_CR_PAYOFF_TYPE cpt on (dc.CR_PAYOFF_TYPE_ID = cpt.id)
       left join ADDRESSES          aa2 on (dc.REAL_ADDRESS_ID = aa2.id  /*and*/ 
                               --             aa2.country_id = rco.id and 
                             --               aa2.city_id = rct1.id /*and*//* 
                                      /*      aa2.REGION_ID = rg1.id and 
                                            aa2.DISTRICT_ID = rd1.id*/) -- факт. прожив
       left join ref_district rd1 on (aa2.DISTRICT_ID = rd1.id)
       left join ref_country        rco on (aa2.DISTRICT_ID = rd1.id)
       left join REF_CITY           rct1 on (rb.city_id=rct.id)
       left join Doc_Check_Abis_Acc dcaa on (dc.check_abis_acc_id = dcaa.id)
       
 --      REF_DISTRICT       rd1,
       left join REF_REGION         rg1 on (aa2.REGION_ID = rg1.id)
       left join PKB_VALID_CREDITS pvc on pvc.doc_cl_id = dc.id 
       
    where dc.id =   $P{Идентификатор} and rownum = 1]]>
	</queryString>
