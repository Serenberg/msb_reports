decode (dc.card_type_id,
               'PASSPORT', 'Паспорт №',
               'CERTIFICATE', 'жеке куәлік №') as card_type_kaz,
up1.position as mana_pos,
decode(dc.card_by_id, 
'MVD_RK', 'МВД РК',
'MU_RK', 'МЮ РК') as card_by_rus,
decode(dc.card_by_id, 
'MVD_RK', 'ҚР ІІМ ',
'MU_RK', 'ҚР ӘM') as card_by_kaz,
-- ФИО   from doc_cl dc
       dc.term_spec_cond as spec_cond , --спец условия
decode(rp.id, 'REFIN_AB_CONS', 'рефинансирование и другие потребительские цели', 
              'REFIN_CONS', 'рефинансирование и другие потребительские цели',
              'REFIN', 'рефинансирование',
              'CONS', 'потребительские цели') as loan_purpose_rus,
decode(rp.id, 'REFIN_AB_CONS', 'қайта қаржыландыру және тұтынушылық мақсаттарға', 
              'REFIN_CONS', 'қайта қаржыландыру және тұтынушылық мақсаттарға',
              'REFIN', 'қайта қаржыландыру',
              'CONS', 'тұтынушылық мақсаттарға') as loan_purpose_kaz,                           
rp.caption as loan_purp,
decode (cpt.id, 'ANNUITY_PAY', ' аннуитеттік',
                'DIFFERENTIATED_PAY', ' дифференцирленген') as loan_type_kz
,(case when dc.is_real_address > 0 then  get_address(dc.address_id) else  get_address(dc.real_address_id) end) as adr_fact, -- город+область+район+улица+дом
get_address(dc.address_id) as adr_propis,  -- адрес прописки
(select count(*)-1 from doc_cl cb1 where cb1.id <> dc.id and dc.group_reference = cb1.group_reference) as soz_count,
---------------------------------------------------------
--Инфо о созаемщике

(select distinct listagg(chr(10)|| 'и ' || '<b>'||cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') ||
 ' (' || decode (cb.card_type_id, 'PASSPORT', 'Паспорт гр РК №', 'CERTIFICATE', 'Удостоверение личности №') || 
 card_number || ' выдан ' || decode(cb.card_by_id, 'MVD_RK', 'МВД РК', 'MU_RK', 'МЮ РК') || ' от ' || to_char(card_issue_date, 'dd.mm.yyyy') || '-' || to_char(card_end_date, 'dd.mm.yyyy') || ')</b>', ', '|| chr(10)  )
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference) as FIOB_1_RUS, --фио удос рус



(select distinct listagg(chr(10) || 'және <b>' || cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') ||
 ' (' || decode (cb.card_type_id, 'PASSPORT', 'Паспорт №',  'CERTIFICATE', 'жеке куәлік №') || card_number|| ' ' || decode(cb.card_by_id, 'MVD_RK', 'ҚР ІІМ ', 'MU_RK', 'ҚР ӘM') || ' берілген ' || to_char(card_issue_date, 'dd.mm.yyyy') || '-' || to_char(card_end_date, 'dd.mm.yyyy') || ')</b>', ', '|| chr(10) )
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference
                       and cb.group_reference is not null) as FIOB_1_KAZ,  --фио удос каз
                       
                     
                       
                       
 
(select distinct listagg('Созаемщик: ' ||cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') || chr(10) ||
'ИИН ' || cb.iin || chr(10) ||
