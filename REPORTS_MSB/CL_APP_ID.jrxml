 ||  'Адрес регистрации: ' || get_address(cb.address_id) || chr(10) || 'Адрес фактического проживания: ' || 
 (case when cb.  is_real_address > 0  then get_address(cb.address_id) else  get_address(cb.real_address_id) end) || chr(10) ||
 'Тел: ' || cb.mobile_phone1 || ' '|| nvl(cb.mobile_phone2, '') || chr(10) 
||  'Текущий счет : ' || b1.loan_acc  || chr(10) , chr(10))
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       left join reg_cr_abis b1 on (b1.id = cb.reg_cr_abis_id)
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference
						and cb.group_reference is not null) as FIOB_2_RUS, -- фио удос иин адрес телефон рус
                       
(select distinct listagg('Бірге қарыз алушы: '  ||cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') || chr(10) ||
'ЖСН ' || cb.iin || chr(10) ||
 'Жеке басын куәландыратын құжат – ' || decode (cb.card_type_id, 'PASSPORT', 'Паспорт №',  'CERTIFICATE', 'жеке куәлік №') || cb.card_number || ' ' || decode(cb.card_by_id, 'MVD_RK', 'ҚР ІІМ ', 'MU_RK', 'ҚР ӘM') ||' берілген ' || to_char(cb.card_issue_date, 'dd.mm.yyyy') || '-' || to_char(cb.card_end_date, 'dd.mm.yyyy') || chr(10) ||
 'Тіркеу мекен жайы: ' || get_address(cb.address_id) || chr(10) || 'Тұрғылықты мекен жайы: ' ||  (case when cb.  is_real_address > 0  then get_address(cb.address_id) else  get_address(cb.real_address_id) end) || chr(10) ||
 'Тел: ' || cb.mobile_phone1 || ' ' || nvl(cb.mobile_phone2, '') || chr(10)
|| 'Ағымдағы шот: ' || b1.loan_acc || chr(10) , chr(10)) 
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                        left join reg_cr_abis b1 on (b1.id = cb.reg_cr_abis_id)
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference
                        and cb.group_reference is not null) as FIOB_2_KAZ, -- фио удсо иин адрес телефон каз
                       
 
(select distinct listagg(chr(10) || 'Бірге қарыз алушыдан / От Созаемщика:   _________________________'  ||cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') || chr(10),
 ' ')
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference) as FIOB_1_I, -- подписанты

(select distinct listagg(chr(10) || 'Бірге Қарыз алушының атауы / Наименование Созаемщика: '  ||cb.last_name || ' ' || cb.first_name || ' ' || nvl(cb.middle_name, '') || chr(10)
 || 'Жеке куәлігі (төлқұжат)/ Удостоверение личности (паспорт) :' || chr(10) ||
 decode (cb.card_type_id, 'PASSPORT', 'Паспорт №',  'CERTIFICATE', 'жеке куәлік №') || cb.card_number || ' ' || decode(cb.card_by_id, 'MVD_RK', 'МВД РК', 'MU_RK', 'МЮ РК') || ' от ' || to_char(cb.card_issue_date, 'dd.mm.yyyy') || '-' || to_char(cb.card_end_date, 'dd.mm.yyyy') || chr(10) ||
 'Бірге Қарыз алушы ЖСН / ИИН Созаемщика: ' || cb.iin
 , '')
 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       left join reg_cr_abis b1 on ( cb.check_abis_acc_id = b1.id)
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference
                                              and cb.group_reference is not null) as FIOB_2_I, --фио удос иин


 (select distinct listagg(chr(10) || 'Бірге Қарыз алушынын ағымдағы шоты / Текущий счет Созаемщика: ' || nvl(b1.loan_acc, '') , '')
 	 WITHIN GROUP(order by cb.group_reference) over(partition by cb.group_reference) as ename_list
                       from doc_cl cb
                       left join reg_cr_abis b1 on ( cb.check_abis_acc_id = b1.id)
                       where cb.id <> dc.id
                       and cb.group_reference = dc.group_reference
                                              and cb.group_reference is not null) as FIOB_ACC_I,
